describe("", function(){
  it("minimaGananciaPositiva devuelve la ganancia mas baja de las positivas", function() {
    assert.equal(minimaGananciaPositiva([{ mes: "enero", ganancia: -40 }, { mes: "febrero", ganancia: 42 }, { mes: "marzo", ganancia: 19}, { mes: "abril", ganancia:  -5}]), 19) })

  it("minimaGananciaPositiva devuelve la unica ganancia positiva si solo tiene una", function() {
    assert.equal(minimaGananciaPositiva([{ mes: "enero", ganancia: -40 }, { mes: "febrero", ganancia: 42 }, { mes: "marzo", ganancia: -19}, { mes: "abril", ganancia:  -5}]), 42) })
})